<h1>New robot</h1>
<%= error_messages_for :robot %>

<% form_for(:robot, @robot, :url => {:action => 'create'}) do |f| %>
  <p>
    Title: <%= f.text_field 'title' %>
  </p>
  <p>
    Body: <%= f.text_area 'body' %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', :action => nil %>