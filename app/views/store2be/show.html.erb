<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="organization show">
    <h2>Show Organization</h2>

    <table summary="organization detail view">
         <tr>
  <th>title</th>
  <td><%=  @organization.title %></td>
         </tr>
           <tr>
  <th>Description</th>
  <td><%= @organization.Description %></td>
         </tr>
          <tr>
  <th>Type</th>
  <td><%= @organization.Type ? 'true' : 'false' %></td>
          </tr>
          <tr>
  <th>Created</th>
  <td><%= @organization.created_at  %></td>
            </tr>
          <tr>
  <th>Updated</th>
  <td><%= @organization.updated_at  %></td>
           </tr>
          </table>

</div>